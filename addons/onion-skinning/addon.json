{
  "name": "Onion skinning",
  "description": "Shows transparent overlays of previous costumes when editing a costume.",
  "credits": [
    {
      "name": "GarboMuffin"
    }
  ],
  "userscripts": [
    {
      "url": "userscript.js",
      "matches": ["https://scratch.mit.edu/projects/*"]
    }
  ],
  "userstyles": [
    {
      "url": "userscript.css",
      "matches": ["https://scratch.mit.edu/projects/*"]
    }
  ],
  "settings": [
    {
      "id": "default",
      "name": "Enable by default",
      "type": "boolean",
      "default": false
    },
    {
      "id": "layers",
      "name": "Layers",
      "type": "positive_integer",
      "default": 1
    },
    {
      "id": "opacity",
      "name": "Opacity levels",
      "type": "string",
      "default": "0.25,0.15,0.05"
    }
  ],
  "tags": ["editor", "beta"],
  "enabledByDefault": false,
  "l10n": true
}
