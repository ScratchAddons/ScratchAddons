{
  "name": "Editor modes",
  "description": "Adds the 90s, Old timey and Prehistoric modes from April Fools 2022 and the Mystery Mode from 2020.",
  "credits": [
    {
      "name": "Maximouse",
      "link": "https://scratch.mit.edu/users/Maximouse/"
    }
  ],
  "versionAdded": "1.44.0",
  "info": [
    {
      "type": "notice",
      "id": "performance",
      "text": "The Prehistoric and Mystery modes may impact performance when the editor is open."
    }
  ],
  "dynamicEnable": true,
  "dynamicDisable": true,
  "updateUserstylesOnSettingsChange": true,
  "userscripts": [
    {
      "url": "userscript.js",
      "matches": ["projects"]
    }
  ],
  "userstyles": [
    {
      "url": "common.css",
      "matches": ["projects"]
    },
    {
      "url": "90s.css",
      "matches": ["projects"],
      "if": {
        "settings": { "mode": "90s" }
      }
    },
    {
      "url": "oldtimey.css",
      "matches": ["projects"],
      "if": {
        "settings": { "mode": "oldTimey" }
      }
    },
    {
      "url": "prehistoric.css",
      "matches": ["projects"],
      "if": {
        "settings": { "mode": "prehistoric" }
      }
    },
    {
      "url": "mystery.css",
      "matches": ["projects"],
      "if": {
        "settings": { "mode": "mystery" }
      }
    }
  ],
  "settings": [
    {
      "id": "mode",
      "type": "select",
      "name": "Mode",
      "potentialValues": [
        {
          "id": "90s",
          "name": "90s"
        },
        {
          "id": "oldTimey",
          "name": "Old timey"
        },
        {
          "id": "prehistoric",
          "name": "Prehistoric"
        },
        {
          "id": "mystery",
          "name": "Mystery"
        }
      ],
      "default": "90s"
    }
  ],
  "tags": ["editor", "easterEgg"],
  "relatedAddons": ["cat-blocks"],
  "libraries": ["scratch-gui"]
}
