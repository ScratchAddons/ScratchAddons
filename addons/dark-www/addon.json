  "dynamicEnable": true,
  "dynamicDisable": true,
  "injectAsStyleElt": true,
  "versionAdded": "1.4.0",
  "latestUpdate": {
    "version": "1.23.0",
    "isMajor": true,
    "temporaryNotice": "You can now completely customize the appearance of the Scratch website."
  },
  "updateUserstylesOnSettingsChange": true,
  "userscripts": [
    {
      "url": "classes.js",
      "matches": ["https://scratch.mit.edu/annual-report/*"],
      "runAtComplete": false
    },
    {
      "url": "experimental_forums_preview.js",
      "matches": ["editingScreens"]
    }
  ],
  "userstyles": [
    {
      "url": "scrollbar.css",
      "matches": ["https://scratch.mit.edu/*"],
      "if": {
        "settings": { "darkScrollbars": true }
      }
    },
    {
      "url": "experimental_scratchr2.css",
      "matches": ["isNotScratchWWW"]
    },
    {
      "url": "experimental_scratchwww.css",
      "matches": ["scratchWWWNoProject", "projects"]
    },
    {
      "url": "experimental_scratchwww_white_bg.css",
      "matches": ["https://scratch.mit.edu/ideas", "https://scratch.mit.edu/annual-report"]
    },
    {
      "url": "darker_banners.css",
      "matches": ["scratchWWWNoProject"],
      "if": {
        "settings": { "darkBanners": "darker" }
      }
    },
    {
      "url": "desaturated_banners.css",
      "matches": ["scratchWWWNoProject"],
      "if": {
        "settings": { "darkBanners": "desaturated" }
      }
    },
    {
      "url": "blue_banners_scratchr2.css",
      "matches": ["isNotScratchWWW"],
      "if": {
        "addonEnabled": ["scratchr2"]
      }
    },
    {
      "url": "darker_banners_scratchr2.css",
      "matches": ["isNotScratchWWW"],
      "if": {
        "settings": { "darkBanners": "darker" },
        "addonEnabled": ["scratchr2"]
      }
    },
    {
      "url": "desaturated_banners_scratchr2.css",
      "matches": ["isNotScratchWWW"],
      "if": {
        "settings": { "darkBanners": "desaturated" },
        "addonEnabled": ["scratchr2"]
      }
    },
    {
      "url": "experimental_search.css",
      "matches": ["https://scratch.mit.edu/explore/*", "https://scratch.mit.edu/search/*"]
    },
    {
      "url": "explore_desaturated_banner.css",
      "matches": ["https://scratch.mit.edu/explore/*"],
      "if": {
        "settings": { "darkBanners": "desaturated" }
      }
    },
    {
      "url": "search_desaturated_banner.css",
      "matches": ["https://scratch.mit.edu/search/*"],
      "if": {
        "settings": { "darkBanners": "desaturated" }
      }
    },
    {
      "url": "experimental_project_embed.css",
      "matches": ["projectEmbeds"]
    },
    {
      "url": "pygments.css",
      "matches": ["forums"],
      "if": {
        "settings": { "darkForumCode": true }
      }
    }
  ],
