{
  "name": "3.0 Scratchblocks on forums",
  "description": "Enables the new version of scratchblocks on the forums.",
  "credits": [
    {
      "name": "Verixion",
      "link": "https://scratch.mit.edu/users/Verixion/"
    },
    {
      "name": "CubeyTheCube",
      "link": "https://github.com/CubeyTheCube"
    },
    {
      "name": "apple502j",
      "link": "https://github.com/apple502j"
    }
  ],
  "userscripts": [
    {
      "url": "userscript.js",
