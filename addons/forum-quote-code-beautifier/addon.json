{
  "name": "Customized quotes & code blocks on forums",
  "description": "Shows nicer code and quote blocks on the forums.",
  "credits": [
    {
      "name": "GrahamSH (Port)"
    },
    {
      "name": "Boomer001 (Userstyle)"
    }
  ],
  "settings": [
    {
      "name": "Border Color",
      "id": "bordercolor",
      "type": "color",
      "default": "#28A5DA",
      "allowTransparency": true
    }
  ],
  "dynamicEnable": true,
  "dynamicDisable": true,
  "injectAsStyleElt": true,
  "userstyles": [
    {
      "url": "main.css",
      "matches": ["https://scratch.mit.edu/discuss/*"]
    }
  ],
  "tags": ["forums", "theme"],
  "l10n": true
}
