{
  "name": "Customizable navigation bar",
  "description": "Choose a list of links to be displayed in the navigation bar.",
  "userscripts": [
    {
      "url": "userscript.js",
      "matches": ["https://scratch.mit.edu/*"],
      "runAtComplete": false
    }
  ],
  "settings": [
    {
      "name": "Items",
      "id": "items",
      "type": "table",
      "default": [
        ["Create", "/projects/editor/"],
        ["Explore", "/explore/projects/all"],
        ["Ideas", "/ideas"],
        ["About", "/about"]
      ],
      "row": [
        {
          "name": "Name",
          "id": "name",
          "type": "string"
        },
        {
          "name": "URL",
          "id": "url",
          "type": "string"
        }
      ]
    }
  ],
  "versionAdded": "1.23.0",
  "tags": ["community", "recommended"],
  "enabledByDefault": false
}
