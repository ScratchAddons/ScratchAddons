{
  "name": "Customizable navigation bar",
  "description": "Choose a list of links to be displayed in the navigation bar.",
  "userscripts": [
    {
      "url": "userscript.js",
      "matches": ["https://scratch.mit.edu/*"],
      "runAtComplete": false
    },
    {
      "url": "www.js",
      "matches": ["scratchWWWNoProject", "projects"],
      "runAtComplete": false
    },
    {
      "url": "r2.js",
      "matches": ["isNotScratchWWW"],
      "runAtComplete": false
    }
  ],
  "userstyles": [
    {
      "url": "compact-nav.css",
      "matches": ["https://scratch.mit.edu/*"],
      "if": {
        "settings": {
          "compact-nav": true
        }
      }
    },
    {
      "url": "stick.css",
      "matches": ["https://scratch.mit.edu/*"],
      "if": {
        "settings": {
          "stick": "page"
        }
      }
    }
  ],
  "settings": [
    {
      "name": "Items",
      "id": "items",
      "type": "table",
      "default": [
        { "name": "Create", "url": "/projects/editor/" },
        { "name": "Explore", "url": "/explore/projects/all" },
        { "name": "Ideas", "url": "/ideas" },
        { "name": "About", "url": "/about" }
      ],
      "row": [
        {
          "name": "Name",
          "id": "name",
          "type": "string",
          "default": ""
        },
        {
          "name": "URL",
          "id": "url",
          "type": "untranslated",
          "default": ""
        }
      ],
      "presets": [
        {
          "name": "Create",
          "values": { "name": "Create", "url": "/projects/editor/" }
        },
        {
          "name": "Explore",
          "values": { "name": "Explore", "url": "/explore/projects/all" }
        },
        {
          "name": "Ideas",
          "values": { "name": "Ideas", "url": "/ideas" }
        },
        {
          "name": "About",
          "values": { "name": "About", "url": "/about" }
        },
        {
          "name": "Discuss",
          "values": { "name": "Discuss", "url": "/discuss" }
        },
        {
          "name": "Wiki",
          "values": { "name": "Wiki", "url": "https://scratch-wiki.info/" }
        }
      ]
    },
    {
      "name": "Compact user dropdown",
      "id": "compact-nav",
      "type": "boolean",
      "default": false
    },
    {
      "name": "Stick to",
      "id": "stick",
      "type": "select",
      "default": "screen",
      "potentialValues": [
        {
          "id": "screen",
          "name": "Top of screen"
        },
        {
          "id": "page",
          "name": "Top of page"
        }
      ]
    }
  ],
  "dynamicEnable": true,
  "dynamicDisable": true,
  "versionAdded": "1.0.0",
  "latestUpdate": {
    "version": "1.32.0",
    "newSettings": ["stick"],
    "isMajor": false
  },
  "tags": ["community", "featured"],
  "enabledByDefault": false,
  "updateUserstylesOnSettingsChange": true
}
