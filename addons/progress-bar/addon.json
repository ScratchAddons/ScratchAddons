{
  "name": "Project progress bar",
  "description": "Adds a progress bar for loading and saving projects.",
  "credits": [
    {
      "name": "GarboMuffin"
    },
    {
      "name": "jbthepig",
      "link": "https://scratch.mit.edu/users/jbthepig/"
    }
  ],
  "userscripts": [
    {
      "url": "userscript.js",
      "matches": ["projects"],
      "runAtComplete": false
    }
  ],
  "userstyles": [
    {
      "url": "userstyle.css",
      "matches": ["projects"]
    }
  ],
  "settings": [
    {
      "name": "Show progress bar at top of page",
      "id": "topbar",
      "type": "boolean",
      "default": false
    },
    {
      "name": "Progress bar height (px)",
      "id": "height",
      "type": "integer",
      "min": 3,
      "max": 25,
      "default": 8
    }
  ],
  "dynamicEnable": true,
  "dynamicDisable": true,
  "versionAdded": "1.0.0",
  "tags": ["editor", "recommended"]
}
