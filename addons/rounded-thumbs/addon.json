{
  "name": "Rounded thumbnail corners",
  "description": "Makes the corners of project and studio thumbnails round.",
  "credits": [
    {
      "name": "DNin01",
      "link": "https://github.com/DNin01"
    }
  ],
  "tags": ["community", "theme"],
  "dynamicEnable": true,
  "dynamicDisable": true,
  "injectAsStyleElt": true,
  "updateUserstylesOnSettingsChange": true,
  "versionAdded": "1.31.0",
  "userstyles": [
    {
      "url": "scratch3.css",
      "matches": ["https://scratch.mit.edu/"]
    },
    {
      "url": "morescratch3.css",
      "matches": ["https://scratch.mit.edu/search/*", "https://scratch.mit.edu/explore/*"]
    },
    {
      "url": "profile.css",
      "matches": [
        "https://scratch.mit.edu/users/*",
        "https://scratch.mit.edu/projects/*/remixes/",
        "https://scratch.mit.edu/projects/*/studios/"
      ]
    },
    {
      "url": "mystuff.css",
      "matches": ["https://scratch.mit.edu/mystuff/*"]
    },
    {
      "url": "studios.css",
      "matches": ["https://scratch.mit.edu/studios/*"]
    },
    {
      "url": "projects.css",
      "matches": ["https://scratch.mit.edu/projects/*"]
    }
  ],
  "settings": [
    {
      "name": "Corner radius (px)",
      "id": "round-size",
      "type": "integer",
      "min": 0,
      "max": 15,
      "default": 6
    }
  ]
}
