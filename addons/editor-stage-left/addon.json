{
  "name": "Display stage on left side",
  "description": "Moves the stage to the left side of the editor.",
  "credits": [
    {
      "name": "NitroCipher/ZenithRogue"
    },
    {
      "name": "djdolphin"
    }
  ],
  "userscripts": [
    {
      "url": "fix-share-the-love.js",
      "matches": ["https://scratch.mit.edu/projects/*"]
    }
  ],
  "dynamicDisable": true,
  "userstyles": [
    {
      "url": "stageleft.css",
      "matches": ["https://scratch.mit.edu/projects/*"]
    },
    {
      "url": "fullscreen-on-left.css",
      "matches": ["https://scratch.mit.edu/projects/*"],
      "settingMatch": {
        "id": "fullscreenOnLeft",
        "value": true
      }
    }
  ],
  "settings": [
    {
      "name": "Fullscreen button on left",
      "id": "fullscreenOnLeft",
      "type": "boolean",
      "default": false
    }
  ],
  "tags": ["editor", "theme"],
  "enabledByDefault": false,
  "l10n": true
}
