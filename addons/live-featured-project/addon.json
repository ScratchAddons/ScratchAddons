{
  "name": "Live featured project",
  "description": "Shows an interactive featured project, directly from the user's profile. Based on the implementation in Mega Scratch Userscript.",
  "credits": [
    {
      "name": "GRA0007",
      "note": "original",
      "id": "original",
      "link": "https://github.com/MegaScratchUserscript/Mega-Scratch-Userscript/blob/master/originals/GRA0007/scratchultimate.user.js#L23-L46"
    },
    {
      "name": "Hans5958",
      "note": "remake",
      "id": "remake"
    },
    {
      "name": "ErrorGamer2000",
      "link": "https://github.com/ErrorGamer2000",
      "note": "update",
      "id": "update"
    }
  ],
  "info": [
    {
      "id": "privacy",
      "type": "warning",
      "text": "Enabling the \"Use Scratch username\" when using the TurboWarp player will give TurboWarp access to your Scratch username."
    }
  ],
  "userscripts": [
    {
      "url": "script.js",
      "matches": ["https://scratch.mit.edu/users/*"]
    }
  ],
  "userstyles": [
    {
      "url": "style.css",
      "matches": ["https://scratch.mit.edu/users/*"]
    }
  ],
  "settings": [
    {
      "id": "showMenu",
      "name": "Show project menu (top bar)",
      "type": "boolean",
      "default": false
    },
    {
      "id": "alternativePlayer",
      "name": "Project player",
      "type": "select",
      "default": "none",
      "potentialValues": [
        {
          "id": "none",
          "name": "Scratch"
        },
        {
          "id": "turbowarp",
          "name": "TurboWarp"
        },
        {
          "id": "forkphorus",
          "name": "forkphorus"
        }
      ]
    },
    {
      "id": "autoPlay",
      "name": "Auto-play projects",
      "type": "boolean",
      "default": false,
      "if": {
        "settings": { "alternativePlayer": ["turbowarp", "forkphorus"] }
      }
    },
    {
      "id": "enableTWAddons",
      "name": "Enable TurboWarp addons",
      "type": "boolean",
      "default": true,
      "if": {
        "settings": { "alternativePlayer": ["turbowarp"] }
      }
    },
    {
      "id": "shareUsername",
      "name": "Use Scratch username",
      "type": "boolean",
      "default": true,
      "if": {
        "settings": { "alternativePlayer": ["turbowarp"] }
      }
    }
  ],
  "tags": ["community", "profiles", "featured"],
  "versionAdded": "1.3.0",
  "enabledByDefault": false
}
